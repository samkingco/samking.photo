{
  "name": "samking.photo",
  "version": "1.0.0",
  "description": "Site for info about my photography",
  "scripts": {
    "copystatic": "copyfiles -f src/static/* dist",
    "dev": "parcel serve src/index.html -p 3000",
    "build": "cross-env PARCEL_WORKERS=0 parcel build src/index.html && npm run copystatic"
  },
  "staticFiles": {
    "staticPath": [
      "avatar.jpg",
      "robots.txt"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samkingco/samking.photo.git"
  },
  "keywords": [
    "eth",
    "ipfs"
  ],
  "author": "Sam King",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samkingco/samking.photo/issues"
  },
  "homepage": "https://github.com/samkingco/samking.photo#readme",
  "devDependencies": {
    "@parcel/config-default": "^2.0.0-rc.0",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "parcel": "^2.0.0-rc.0",
    "sharp": "^0.29.1"
  }
}
